{
    "name": "salehhashemi/laravel-intelli-db",
    "description": "An AI-driven Laravel package for generating and managing database related things.",
    "keywords": [
        "salehhashemi",
        "laravel",
        "intelli-db",
        "ai",
        "database",
        "factories",
        "migrations",
        "models",
        "rules",
        "seeders"
    ],
    "homepage": "https://github.com/salehhashemi1992/laravel-intelli-db",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Saleh Hashemi",
            "email": "salpars2004@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.8",
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Salehhashemi\\LaravelIntelliDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Salehhashemi\\LaravelIntelliDb\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Salehhashemi\\LaravelIntelliDb\\LaravelIntelliDbServiceProvider"
            ],
            "aliases": {
                "LaravelIntelliDb": "Salehhashemi\\LaravelIntelliDb\\LaravelIntelliDbFacade"
            }
        }
    }
}
